{
  "name": "JiraHighlighter",
  "description": "Highlights Jira Elements based on your query selector",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "jh.png"
  },
  "action": {
    "default_title": "JiraHighlighter",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "jh.png"
  }
}