{
  "name": "Highlighter",
  "description": "Highlights HTML Elements based on your query selector",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "ui/scripts/background.js"
  },
  "action": {
    "default_title": "Highlighter",
    "default_popup": "ui/html/popup.html",
    "default_icon": "images/h.png"
  },
  "icons": {
    "16": "images/h.png"
  }
}